<+>{% load static %}\r\n\r\n\r\n<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Мой макет</title>\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\r\n    <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css\" rel=\"stylesheet\"\r\n          integrity=\"sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6\" crossorigin=\"anonymous\">\r\n\r\n    <link rel=\"stylesheet\" href=\"{% static 'store/css/style.css' %}\">\r\n</head>\r\n<div class=\"verx\">\r\n    <div class=\"dostavka_verx\">Доставка та оплата</div>\r\n    <div class=\"vakansiy_verx\">Вакансії</div>\r\n    <div class=\"pro_verx\" >Про нас</div>\r\n    <div class=\"spivrobitnuchtvo_verx\" >Співробітництво</div>\r\n    <select id=\"languageDropdown\">\r\n  <option value=\"en\">English</option>\r\n  <option value=\"fr\">Français</option>\r\n  <!-- Добавьте остальные языки по мере необходимости -->\r\n</select>\r\n    <div class=\"korzuna_verx\"><img class=\"corzina_img_verx\" src=\"{% static 'store/img/corzina.png' %}\"></div>\r\n\r\n<header>\r\n    <div class=\"\">\r\n        <div class=\"header\">\r\n            <div class=\"rounded-gradient-border container_logo\">\r\n                <div class=\"rounded-logo\">\r\n                    <img class=\"imm\" src=\"{% static 'store/img/logoi.svg' %}\" alt=\"logo\">\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</header>\r\n</div>\r\n<body>\r\n\r\n\r\n</div>\r\n\r\n<div class=\"image-button\">\r\n    <img src=\"{% static 'store/img/solar_cart-bold.svg' %}\" alt=\"Кнопка с изображением\">\r\n\r\n</div>\r\n\r\n<div class=\"search-panel\">\r\n    <input type=\"text\" placeholder=\"Search\">\r\n    <a src=\"{% static 'store/img/search.svg' %}\" alt=\"Кнопка с изображением\"></a>\r\n\r\n</div>\r\n\r\n\r\n\r\n\r\n<div class=\"polosha\">\r\n\r\n    <img src=\"{% static 'store/img/sushi.png' %}\" alt=\"\" srcset=\"\" class=\"sushi\" ID=\"sushi\">\r\n    <a class=\"sushi1\" id='sushi11'>Суші</a>\r\n    <img src=\"{% static 'store/img/Vector (2).png' %}\" alt=\"\" srcset=\"\" class=\"pizza\">\r\n    <div class=\"pizza1\">Пица</div>\r\n    <img src=\"{% static 'store/img/Vector (3).png' %}\" alt=\"\" srcset=\"\" class=\"salate\">\r\n    <div class=\"salate1\">Салати</div>\r\n    <img src=\"{% static 'store/img/Vector (4).png' %}\" alt=\"\" srcset=\"\" class=\"hot\">\r\n    <div class=\"hot1\">Горячие</div>\r\n    <img src=\"{% static 'store/img/Vector (5).png' %}\" alt=\"\" srcset=\"\" class=\"basis\">\r\n    <div class=\"basis1\">Основні</div>\r\n    <img src=\"{% static 'store/img/tarelkaess.png' %}\" alt=\"\" srcset=\"\" class=\"soup\">\r\n    <div class=\"soup1\">Супы</div>\r\n    <img src=\"{% static 'store/img/Vector (11).png' %}\" alt=\"\" srcset=\"\" class=\"snacks\">\r\n    <div class=\"snacks1\">Закуски</div>\r\n    <img src=\"{% static 'store/img/Vector (12).png' %}\" alt=\"\" srcset=\"\" class=\"garnish\">\r\n    <div class=\"garnish1\">Гарніри</div>\r\n    <img src=\"{% static 'store/img/Vector (13).png' %}\" alt=\"\" srcset=\"\" class=\"nopoi\">\r\n    <div class=\"nopoi1\">Напої</div>\r\n    <img src=\"{% static 'store/img/puvo_ess.png' %}\" alt=\"\" srcset=\"\" class=\"puvo\">\r\n    <div class=\"puvo1\">Пиво</div>\r\n</div>\r\n\r\n<div class=\"container-fluid my-carousel my-image1\">\r\n    <div id=\"carouselExampleIndicators\" class=\"carousel slide carousel-fade\" data-bs-ride=\"carousel\"\r\n         data-bs-interval=\"5000\">\r\n        <div class=\"carousel-inner\">\r\n            <div class=\"carousel-item active\">\r\n                <img src=\"{% static 'store/img/photo (1)2.png' %}\" class=\"d-block w-100\" alt=\"...\">\r\n            </div>\r\n            <div class=\"carousel-item\">\r\n                <img src=\"{% static 'store/img/photo (1).png' %}\" class=\"