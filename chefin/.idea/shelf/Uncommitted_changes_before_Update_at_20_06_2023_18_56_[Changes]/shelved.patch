<+>{% load static %}\r\n\r\n<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <title>rolu</title>\r\n    <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css\" rel=\"stylesheet\"\r\n          integrity=\"sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6\" crossorigin=\"anonymous\">\r\n\r\n    <link rel=\"stylesheet\" href=\"{% static 'store/css/rolu.css' %}\">\r\n</head>\r\n<body>\r\n<div class=\"carta_tovara\">\r\n    <div class=\"row\">\r\n        {% for product in products %}\r\n            {% if product.category == 'Роли' %}\r\n                <div class=\"col-md-3\">\r\n                    <div class=\"carta_tovara_item\" id=\"rolu_carta\">\r\n                        <img class=\"card-img-top\" src=\"{{ product.image }}\" alt=\"\">\r\n                        <div class=\"card-body\">\r\n                            <h4 class=\"card-title my-title\">\r\n                                <a>{{ product.name }}</a>\r\n                            </h4>\r\n                            <div class=\"price_and_add\">\r\n                                <h5>{{ product.price_for_view }} грн</h5>\r\n                                <a class=\"btn btn-outline-success btnnn\" href=\"#\">\r\n                                    Додати +\r\n                                </a>\r\n                            </div>\r\n                            <p class=\"card-text\">{{ product.description }}</p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            {% endif %}\r\n        {% endfor %}\r\n    </div>\r\n</div>\r\n</body>\r\n</html>
